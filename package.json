{
  "name": "wurd-react",
  "version": "6.0.0",
  "description": "Components to easily integrate the Wurd CMS into your React app",
  "main": "dist/wurd.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "example": "serve examples/simple"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "mocha": "5.2.x",
    "react": "^15.0",
    "react-dom": "^15.0",
    "serve": "9.3.x",
    "webpack": "2.4.1",
    "yargs": "7.1.0"
  },
  "keywords": [
    "cms",
    "headless",
    "content",
    "react"
  ],
  "author": "Charlie Davison",
  "license": "ISC",
  "dependencies": {
    "wurd-web": "git://github.com/wurdcms/wurd-web.git#feature/blocks"
  },
  "peerDependencies": {
    "react": "^15.0"
  },
  "files": [
    "*.md",
    "dist",
    "src",
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wurdcms/wurd-react.git"
  },
  "homepage": "https://github.com/wurdcms/wurd-react"
}
